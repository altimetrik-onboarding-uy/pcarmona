<aura:component controller="ContactWIListController" implements="flexipage:availableForRecordHome,force:hasRecordId" access="global" >
    <aura:attribute name="rows" type="List"/>
    
    <aura:handler name="init" value="{!this}" action="{!c.doInit}" />
    
    <ul class="slds-has-dividers_around-space">
        <aura:iteration items="{!v.rows}" var="wItem">
            <li class="slds-item">
                <c:WItemCard record="{!wItem}" />
            </li>
        </aura:iteration>
    </ul>
    
</aura:component>